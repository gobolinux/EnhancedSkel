Xcursor.theme: neon
Xcursor.size: 24

URxvt.cursorColor: green
URxvt.font: xft:Lode Sans Mono:pixelsize=12
URxvt.boldFont: xft:Lode Sans Mono:pixelsize=12
URxvt.italicFont: xft:Lode Sans Mono:pixelsize=12
URxvt.boldItalicFont: xft:Lode Sans Mono:pixelsize=12
URxvt.depth: 32
URxvt.background: rgba:0000/0000/0000/e5bb
URxvt.foreground: #bcc
URxvt.scrollBar: true
URxvt.scrollBar_right: true
URxvt.scrollBar_floating: false
URxvt.saveLines: 100000
URxvt.internalBorder: 0
URxvt.termName: rxvt

URxvt.clipboard.autocopy: true
URxvt.keysym.S-Insert: \033[2$
URxvt.keysym.C-S-Up: \033[1;6A
URxvt.keysym.C-S-Down: \033[1;6B
URxvt.keysym.C-S-Right: \033[1;6C
URxvt.keysym.C-S-Left: \033[1;6D
URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.colorBD: #fff
URxvt.colorIT: #ddf
URxvt.scrollColor: #508080
uRxvt*color3: #ffff30
URxvt*color12: #3030ff

URxvt.perl-ext-common: default,matcher,tabbedex
! tabbedex settings
URxvt.tabbed.no-tabbedex-keys: yes
URxvt.tabbed.tabbar-fg: 2
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 3
URxvt.tabbed.tab-bg: 0
URxvt.tabbed.autohide: yes
URxvt.tabbed.new-button: no
URxvt.tabbed.title: yes
URxvt.tabbed.title-fg: 3
URxvt.tabbed.title-bg: 0
URxvt.keysym.C-A-t: perl:tabbedex:new_tab
